{
  "name": "--libraryname--",
  "version": "0.0.0",
  "description": "",
  "author": "--username-- <--email-->",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "typescript"
  ],
  "files": [
    "dist/*"
  ],
  "source": "src/index.ts",
  "main": "./dist/--libraryname--.js",
  "module": "./dist/--libraryname--.m.js",
  "typings": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p tsconfig.json -t stylish 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "microbundle build --compress false --name=trueskill --format cjs,es --target node",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run test -- --coverage --no-cache",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist && semantic-release",
    "postinstall": "ts-node setup.ts && npm install --ignore-scripts"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/inquirer": "0.0.43",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/node": "10.9.4",
    "@types/rimraf": "2.0.2",
    "@types/shelljs": "0.8.0",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "inquirer": "6.2.0",
    "jest": "23.5.0",
    "lodash": "4.17.10",
    "microbundle": "^0.6.0",
    "prettier": "1.14.2",
    "replace-in-file": "3.4.2",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.12",
    "shelljs": "0.8.2",
    "travis-deploy-once": "5.0.7",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-config-standard": "8.0.1",
    "typedoc": "0.12.0",
    "typescript": "3.0.3"
  },
  "release": {
    "branch": "--master--"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
